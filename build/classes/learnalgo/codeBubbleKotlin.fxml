<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="600.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="learnalgo.CodeBubbleKotlinController">
   <children>
      <TextArea editable="false" prefHeight="600.0" prefWidth="700.0" text="import java.util.Comparator&#10; &#10;fun &lt;T&gt; bubbleSort(a: Array&lt;T&gt;, c: Comparator&lt;T&gt;) {&#10;    var changed: Boolean&#10;    do {&#10;        changed = false&#10;        for (i in 0..a.size - 2) {&#10;            if (c.compare(a[i], a[i + 1]) &gt; 0) {&#10;                val tmp = a[i]&#10;                a[i] = a[i + 1]&#10;                a[i + 1] = tmp&#10;                changed = true&#10;            }&#10;        }&#10;    } while (changed)&#10;}&#10;">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </TextArea>
      <Button layoutX="615.0" layoutY="568.0" mnemonicParsing="false" onAction="#backButtonAction" text="BACK" />
   </children>
</AnchorPane>
